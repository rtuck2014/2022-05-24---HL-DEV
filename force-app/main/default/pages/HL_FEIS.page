<apex:page standardController="Opportunity_Approval__c" extensions="HL_FEISController" tabStyle="Approval_Object__c" lightningStyleSheets="true">
    <apex:form title="FEIS" >
        <apex:pageMessages ></apex:pageMessages>
        <apex:actionStatus id="refreshStatus" startText="Refreshing..." stopText="" startStyle="font-weight: bold; font-size: 1.5em;">
            <apex:facet name="start" ><apex:outputPanel ><img src="/img/loading.gif" /><span>&nbsp;Refreshing...</span></apex:outputPanel></apex:facet>
        </apex:actionStatus>
        <apex:sectionHeader title="FEIS" subtitle="{!Opportunity_Approval__c.Related_Opportunity__r.Name}"></apex:sectionHeader>
        <apex:outputLabel value="{!Opportunity_Approval__c.Related_Opportunity__c}" rendered="false"></apex:outputLabel>
        <apex:pageBlock mode="edit">
            <div style="float: right;" class="bold">
                <input id="toggleTabs" type="checkbox"/><span>Toggle Tabs</span>
            </div>
            <table class="instructionalText">
                <tbody><tr>
                    <td class="instructionImg"><img src="/img/msg_icons/info32.png" title="Instructions"/></td>
                    <td class="instructions">
                        <p class="bold center" style="font-size: 1.5em;">
                            FEIS (Part I) Form
                        </p>
                        <p>
                            This form should be prepared or at least reviewed by an officer on the deal team and should be reviewed and approved by the Principal and distributed via e-mail to the FEC as early as possible in the engagement process (before the client is provided with any fee estimate or a draft of the engagement letter) and at least seven days prior to the expected delivery date of the fairness opinion. However, in the case of a fairness opinion that will be rendered as part of an M&amp;A advisory engagement, this form should be be sent only to the Transaction Opinion Administrative Coordinator and the Co-Chairs of the FEC at least seven days prior to the expected delivery date of the fairness opinion. For additional details, please consult the Fairness Opinion Engagement and Issuance Procedures. <br /> <br /> 
                            No member of the deal team may have any material financial interests in, or any directorships, family relationships or other relationships with, the client or any other participant in the transaction that might create an actual or perceived conflict of interest. <br /> <br /> 
                            Any fairness opinion engagement involving a public company should be staffed with at least one officer with sufficient experience in public company transactions.
                        </p>
                        <apex:outputPanel rendered="{!Opportunity_Approval__c.Reviewed__c}">
                            <p style="color:red;">
                                Since this form has previously been sent to the Fairness Engagement Committee, please ensure that any updated version of this form (and, if appropriate, an explanation of the changes) is also sent to the FEC.  If the proposed engagement has already received FEC approval based on the previous version of this form, and the changes that are going to be made are material, the FEC should re-approve the proposed engagement based on the updated version of this form.
                            </p>
                        </apex:outputPanel>
                    </td>
                    </tr>
                </tbody></table>
            <apex:pageBlockButtons style="text-align:center" >
                <apex:outputPanel rendered="{!$Profile.Name <> 'System Administrator' && $Profile.Name <> 'CAO' && (Opportunity_Approval__c.Locked__c || NOT(OnOIT))}">
                    <a href="/{!Opportunity_Approval__c.Related_Opportunity__c}" class="btn" style="text-decoration:none;">Return to Opportunity</a>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!NOT($Profile.Name <> 'System Administrator' && $Profile.Name <> 'CAO' && (Opportunity_Approval__c.Locked__c || NOT(OnOIT)))}">
                    <apex:commandButton id="btnSave" action="{!SaveRecord}" value="Save FEIS (Part I)" rendered="{!NOT(EnableRequirements)}" />
                    <apex:commandButton id="btnReturn" action="{!SaveAndReturn}" value="Return to Opportunity" rendered="{!NOT(EnableRequirements)}" />
                    <apex:commandButton id="btnCancel" action="{!CancelSubmission}" value="Cancel Submission" rendered="{!EnableRequirements}" immediate="true" onclick="return confirm('Are you sure?\nNote: Any changes made will still be saved.')" />
                    <apex:commandButton id="btnSubmit" action="{!TriggerValidation}" value="Submit FEIS (Part I) Form" oncomplete="SubmitForm()" rendered="{!NOT(Opportunity_Approval__c.Locked__c)}"/>
                    <apex:commandButton id="SubmitForm" action="{!SubmitForm}" value="Submit FEIS (Part I) Form" style="display:none;" />               
                </apex:outputPanel>
                    <a href= "{!Opportunity_Approval__c.PDF_View__c}" target="_blank" class="btn" style="text-decoration:none;">PDF View</a> 
            </apex:pageBlockButtons>
            <div id="tabs">
                <ul id="tabsList">
                    <li><a href="#tabs-1">General and Background on Transaction</a></li>
                    <li><a href="#tabs-2">Targets / Subjects and Counterparties</a></li>
                    <li><a href="#tabs-3">Form of Opinion</a></li>
                    <li><a href="#tabs-4">Legal Review Criteria</a></li>
                    <li><a href="#tabs-5">Other Opinion Information</a></li>
                    <li><a href="#tabs-6">Review</a></li>
                </ul>
                <div id="tabs-1">
                    <apex:pageBlockSection columns="1" title="Opportunity Overview">
                        <apex:outputField value="{!Opportunity_Approval__c.Related_Opportunity__r.Name}"></apex:outputField>
                        <apex:outputField value="{!Opportunity_Approval__c.Job_Type__c}"></apex:outputField>
                        <apex:outputField value="{!Opportunity_Approval__c.Client_Company__c}"></apex:outputField>
                        <apex:outputField value="{!Opportunity_Approval__c.Subject_Company__c}"></apex:outputField>
                        <apex:outputField value="{!Opportunity_Approval__c.Opportunity_Number__c}"></apex:outputField>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1" title="Referral Information">
                        <apex:outputField value="{!Opportunity_Approval__c.Referral_Type__c}"></apex:outputField>
                        <apex:outputField value="{!Opportunity_Approval__c.Referral_Source__c}" label="Referral Contact"></apex:outputField>
                    </apex:pageBlockSection> 
                    <apex:pageBlockSection columns="1" title="General">
                        <apex:inputField value="{!Opportunity_Approval__c.Estimated_Fee__c}" label="Proposed Aggregate Opinion Fee"></apex:inputField>
                        <apex:inputField value="{!Opportunity_Approval__c.Retainer__c}" label="Amount Paid Upfront"></apex:inputField>
                        <apex:inputField value="{!Opportunity_Approval__c.Amount_Paid_Upon_Delivery2__c}" label="Amount Paid Upon Delivery" styleClass="numeric-short"></apex:inputField>
                        <apex:outputLabel value="Proposed Incremental Fees (if any) and Description of Additional Services (if any)"></apex:outputLabel>
                        <apex:inputTextarea value="{!Opportunity_Approval__c.Proposed_Incremental_Fees__c}" label="" cols="90" rows="5"></apex:inputTextarea>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1" title="Background on Transaction">
                        <apex:outputLabel id="descriptionOfTransactionTXT" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.Transaction_Overview__c=='','red','')}" value="Transaction Overview"></apex:outputLabel>
                            <apex:inputTextarea id="descriptionOfTransaction" value="{!Opportunity_Approval__c.Transaction_Overview__c}" cols="90" rows="5" label="" styleClass="required">
                                <apex:actionSupport event="onchange" rerender="descriptionOfTransactionTXT"/>
                            </apex:inputTextarea>
                        <apex:outputPanel >
                            <table cellpadding="3" cellspacing="3">
                                <tr>
                                    <td>
                                        <apex:outputLabel id="TransactionTypeTXT" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.Transaction_Type__c=='', 'red','')}" value="Transaction Type"></apex:outputLabel>
                                    </td>
                                    <td>
                                        <apex:inputField id="TransactionType" value="{!Opportunity_Approval__c.Transaction_Type__c}" styleClass="required">
                                            <apex:actionSupport event="onchange" rerender="TransactionTypeTXT"/>
                                        </apex:inputField>
                                    </td>
                                </tr>
                            </table>
                            <table>
                                <tr>
                                    <td class="indent">
                                        <apex:outputLabel id="TransactionType01" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.FEIS_Other_Job_Type_Desc__c=='', 'red','')}" value="Describe Other Transaction Type"></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="indent">    
                                        <apex:inputTextarea id="TransactionType02" value="{!Opportunity_Approval__c.FEIS_Other_Job_Type_Desc__c}" cols="90" rows="5" label="">
                                            <apex:actionSupport event="onchange" rerender="TransactionType01"/>
                                        </apex:inputTextarea>
                                    </td>
                                </tr>
                            </table>
                            <table cellpadding="3" cellspacing="3">
                                <tr>
                                    <td>
                                        <apex:outputLabel id="LegalStructureTXT" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.Legal_Structure__c=='', 'red','')}" value="Legal Structure"></apex:outputLabel>
                                    </td>
                                    <td>
                                        <apex:inputField id="LegalStructure" value="{!Opportunity_Approval__c.Legal_Structure__c}" styleClass="required">
                                            <apex:actionSupport event="onchange" rerender="LegalStructureTXT"/>
                                        </apex:inputField>
                                    </td>
                                </tr>
                            </table>
                            <table>
                                <tr>
                                    <td class="indent">
                                        <apex:outputLabel id="LegalStructure01" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.FEIS_Other_Legal_Structure_Desc__c=='', 'red','')}" value="Describe Other Legal Structure"></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="indent">
                                        <apex:inputTextarea id="LegalStructure02" value="{!Opportunity_Approval__c.FEIS_Other_Legal_Structure_Desc__c}" cols="90" rows="5" label="">
                                            <apex:actionSupport event="onchange" rerender="LegalStructure01"/>
                                        </apex:inputTextarea>
                                    </td>
                                </tr>
                            </table>
                        </apex:outputPanel>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1" title="Form and Amount of Consideration">
                        <apex:outputPanel >
                            <table cellpadding="3" cellspacing="3">
                                <tr>
                                    <td>
                                        <apex:outputLabel id="TransSizeTXT" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.Estimated_Transaction_Size_MM__c=null, 'red','')}" value="Transaction Size (EV) (MM)"></apex:outputLabel>
                                    </td>
                                    <td>
                                        <apex:inputField id="TransSize" value="{!Opportunity_Approval__c.Estimated_Transaction_Size_MM__c}" styleClass="numeric-short required">
                                            <apex:actionSupport event="onchange" rerender="TransSizeTXT"/>
                                        </apex:inputField>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:outputLabel id="EquitySizeTXT" value="Transaction Size (Equity) (MM)"></apex:outputLabel>
                                    </td>
                                    <td>
                                        <apex:inputField value="{!Opportunity_Approval__c.Estimated_Transaction_Size_Equity_MM__c}" styleClass="numeric-short"> </apex:inputField>
                                    </td>
                                </tr>
                            </table>
                            <table>
                                <tr>
                                    <td>
                                        <apex:outputLabel id="FormConsiderTXT" style="font-size: 0.9em; color:{!IF(EnableRequirements && Opportunity_Approval__c.Form_of_Consideration__c=='', 'red','')}" value="Form of Consideration (pick all that apply)" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField id="FormConsider" value="{!Opportunity_Approval__c.Form_of_Consideration__c}" label="" styleClass="required">
                                            <apex:actionSupport event="onchange" rerender="FormConsiderTXT"/>
                                        </apex:inputField>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="indent">
                                        <apex:outputLabel id="OtherFormTXT" style="color:{!IF(EnableRequirements && CONTAINS(Opportunity_Approval__c.Form_of_Consideration__c,'Other') && Opportunity_Approval__c.FEIS_Other_Forms_of_Consid_Desc__c=='', 'red','')}" value="Describe Other Form of Consideration"></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="indent">
                                        <apex:inputTextarea id="OtherForm" value="{!Opportunity_Approval__c.FEIS_Other_Forms_of_Consid_Desc__c}" cols="90" rows="5" label="">
                                                <apex:actionSupport event="onchange" rerender="OtherFormTXT"/>
                                        </apex:inputTextarea>
                                    </td>
                                </tr>
                            </table>
                        </apex:outputPanel>
                    </apex:pageBlockSection>
                </div>
                <div id="tabs-2">
                    <apex:pageBlockSection columns="1" title="Ownership and Advisors of Target/Subject Company(ies)">
                        <center><input type='button' value='Add Target/Subject' id='newClientSubject' class="btn" /></center>
                        <apex:commandButton id="clientsSubjectsRefresh" action="{!RefreshClientsSubjects}" reRender="panClientsSubjects,pbtShareholderCompanies"  status="refreshStatus" oncomplete="DocumentReady();" immediate="true" style="display:none;"/>
                        <apex:outputPanel id="panClientsSubjects">
