                                            <apex:outputField value="{!cs.Outside_Counsel__c}" styleClass="top" label=""/>
                                            <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
                                            <apex:outputLabel value="Financial Advisor(s) (other than Houlihan Lokey)" style="padding-left:5px;"></apex:outputLabel>
                                            <apex:outputField value="{!cs.Financial_Advisors__c}" styleClass="top" label=""/>
                                            <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
                                            <apex:outputLabel value="Describe services being provided by other Financial Advisor(s)" style="padding-left:5px;"></apex:outputLabel>
                                            <apex:outputField value="{!cs.Other_Financial_Advisor_Services__c}" styleClass="top" label=""/>
                                        </apex:pageBlockSection>
                                    </apex:outputPanel>
                                </apex:column>
                            </apex:pageBlockTable>
                        </apex:outputPanel>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1" title="Ownership and Advisors of Counterparty(ies) in the Transaction">
                        <center><input type='button' value='Add Counterparty' id='newCounterparty' class="btn" /></center>
                        <apex:commandButton id="counterpartiesRefresh" action="{!RefreshCounterparties}" reRender="panCounterparties,pbtShareholderCompanies" style="display:none;" status="refreshStatus" oncomplete="DocumentReady();" immediate="true"/>
                        <apex:outputPanel id="panCounterparties">
                            <apex:pageBlockTable value="{!Counterparties}" var="cp">
                                <apex:column width="50px" >
                                    <apex:outputPanel rendered="{!NOT(Opportunity_Approval__c.Locked__c)}">
                                        <a id='editCounterparty' class="pointer underline" style="padding-right: 5px;">Edit</a>
                                        <apex:outputLabel value="{!cp.Id}" styleClass="hidden"></apex:outputLabel>
                                        <apex:commandLink value="Del" action="{!DeleteCounterparty}" onclick="return confirmDelete();">
                                            <apex:param name="p2" assignTo="{!DelId}" value="{!cp.Id}"></apex:param>
                                        </apex:commandLink> 
                                        <!--<apex:outputLink value="{!URLFOR($Action.Opportunity_Client_Subject__c.Delete, cp.Id,[retURL='/apex/HL_FEIS?id=' + OpportunityApproval.Id])}" target="_parent" onClick="return confirmDelete()">Del</apex:outputLink>-->
                                    </apex:outputPanel>
                                </apex:column>
                                <apex:column value="{!cp.Client_Subject__r.Name}" headerValue="Counterparty Company Name"/>
                                <apex:column value="{!cp.Public_Or_Private__c}" headerValue="Public/Private"/>
                                <apex:column value="{!cp.Private_Equity_Ownership__c}" headerValue="Private Equity (%)"/>
                                <apex:column value="{!cp.Market_Cap_MM__c}" headerValue="Market Cap (Mil)"/>
                                <apex:column value="{!cp.ESOP__c}" headerValue="ESOP (%)"/>
                                <apex:column value="{!cp.NFP__c}" headerValue="Not for Profit (%)"/>
                                <apex:column value="{!cp.Insiders__c}" headerValue="Insiders (%)"/>
                                <apex:column headerValue="More Details" width="30px" styleClass="center">
                                    <apex:image styleClass="tipPopupTrigger pointer" alt="More Details" url="{!URLFOR($Resource.HL_Images,'Info.png')}" height="16px" title="More Details" rendered="{!NOT ISBLANK(cp.Outside_Counsel__c) || NOT ISBLANK(cp.Financial_Advisors__c) || NOT ISBLANK(cp.Other_Financial_Advisor_Services__c)}"/>
                                    <apex:outputPanel styleClass="popupDetails" style="display:none;">
                                        <apex:pageBlockSection columns="1" title="Additional Details for {!cp.Client_Subject__r.Name}" collapsible="false" >
                                            <apex:outputLabel value="Outside Counsel - Identify the name(s) of all law firm(s) and attorney(ies)" style="padding-left:5px;"></apex:outputLabel>
                                            <apex:outputField value="{!cp.Outside_Counsel__c}" styleClass="top" label=""/>
                                            <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
                                            <apex:outputLabel value="Financial Advisor(s) (other than Houlihan Lokey)" style="padding-left:5px;"></apex:outputLabel>
                                            <apex:outputField value="{!cp.Financial_Advisors__c}" styleClass="top" label=""/>
                                            <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
                                            <apex:outputLabel value="Describe services being provided by other Financial Advisor(s)" style="padding-left:5px;"></apex:outputLabel>
                                            <apex:outputField value="{!cp.Other_Financial_Advisor_Services__c}" styleClass="top" label=""/>
                                        </apex:pageBlockSection>
                                    </apex:outputPanel>
                                </apex:column>
                            </apex:pageBlockTable>
                        </apex:outputPanel>
                    </apex:pageBlockSection>
                    <div style="padding: 5px 0px 5px 10px;">
                        <apex:outputLabel value="Background on Target/Subject Company(ies) and Counterparty(ies) (including summary financial statements, relevant transaction multiples,etc.)" style="padding: 5px 0px 5px 0px;"></apex:outputLabel>
                        <br /><br />
                        <apex:inputTextarea value="{!Opportunity_Approval__c.Background_On_Additional_Companies__c}" cols="140" rows="5"/>
                    </div>
                    <apex:pageBlockSection columns="1" title="Affiliated Parties Information">
                        <apex:outputPanel >
                            <table cellpadding="3" cellspacing="3">
                                <tr>
                                    <td>
                                        <apex:outputLabel id="affiliatedPartiesTXT" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.Opinion_Parties_Affiliated__c=='', 'red','')}" value="Are any of the parties to the transaction affiliated with each other?"></apex:outputLabel>
                                    </td>
                                    <td>
                                        <apex:inputField id="affiliatedParties" value="{!Opportunity_Approval__c.Opinion_Parties_Affiliated__c}" styleClass="required">
                                            <apex:actionSupport event="onchange" reRender="affiliatedPartiesTXT"/>
                                        </apex:inputField>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <apex:outputLabel id="affiliatedParties_01TXT" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.Opinion_Parties_Affiliated__c=='Yes' && Opportunity_Approval__c.Opinion_Affiliated_Parties_Summary__c=='', 'red','')}" value="Identify the parties and describe the affiliation:"></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <apex:inputTextarea id="affiliatedParties_01" value="{!Opportunity_Approval__c.Opinion_Affiliated_Parties_Summary__c}" label="" rows="5" cols="90">
                                            <apex:actionSupport event="onchange" reRender="affiliatedParties_01TXT"/>
                                        </apex:inputTextarea>
                                    </td>
                                </tr>
                            </table>
                        </apex:outputPanel>
                    </apex:pageBlockSection>
                </div>
                <div id="tabs-3">
                    <apex:pageBlockSection columns="1" title="Form of Opinion">
                        <apex:outputPanel >
                            <table style="width:100%" cellpadding="3" cellspacing="3">
                                <tr>
                                    <td colspan="2" class="bold" style="padding-bottom: 10px;">
                                        With respect to which of the following are we being asked to opine?
                                    </td>    
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField value="{!Opportunity_Approval__c.FEIS_Opine_Option_1__c}" label=""></apex:inputField>
                                    </td>
                                    <td>
                                        <apex:outputLabel value="The consideration to be received by the Unaffiliated Stockholders in the Transaction is fair to them from a financial point of view."></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField value="{!Opportunity_Approval__c.FEIS_Opine_Option_2__c}" label=""></apex:inputField>
                                    </td>
                                    <td>
                                        <apex:outputLabel value="The consideration to be received by the Company in the Transaction is fair to the Company from a financial point of view."></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField value="{!Opportunity_Approval__c.FEIS_Opine_Option_3__c}" label=""></apex:inputField>
                                    </td>
                                    <td>
                                        <apex:outputLabel value="The consideration to be paid by the Company in the Transaction is fair to the Company from a financial point of view."></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField value="{!Opportunity_Approval__c.FEIS_Opine_Option_4__c}" label=""></apex:inputField>
                                    </td>
                                    <td>
                                        <apex:outputLabel value="The Exchange Ratio provided for in the Transaction is fair to the Company from a financial point of view."></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField value="{!Opportunity_Approval__c.FEIS_Opine_Option_5__c}" label=""></apex:inputField>
                                    </td>
                                    <td>
                                        <apex:outputLabel value="The Exchange Ratio provided for in the Transaction is fair to the Unaffiliated Stockholders from a financial point of view."></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <apex:inputField id="Other" value="{!Opportunity_Approval__c.FEIS_Opine_Option_6__c}" label=""></apex:inputField>
                                    </td>
                                    <td>
                                        <apex:outputLabel value="Other"></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <apex:outputLabel id="Other01" style="color:{!IF(EnableRequirements && Opportunity_Approval__c.FEIS_Opine_Option_6__c && Opportunity_Approval__c.Form_of_Opinion_Notes__c=='', 'red','')}" value="Other Form of Opinion Description"></apex:outputLabel>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <apex:inputTextarea id="Other02" value="{!Opportunity_Approval__c.Form_of_Opinion_Notes__c}" cols="90" rows="5" label="">
                                            <apex:actionSupport event="onchange" reRender="Other01"/>
                                        </apex:inputTextarea>
                                    </td>
                                </tr>
                            </table>
                        </apex:outputPanel>
                    </apex:pageBlockSection>
                </div>
                <div id="tabs-4">
                    <apex:pageBlockSection columns="1" title="Legal Review Criteria">
                        <apex:outputPanel >
