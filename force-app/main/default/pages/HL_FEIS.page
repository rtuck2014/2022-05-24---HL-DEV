                                    <apex:outputPanel rendered="{!NOT(Opportunity_Approval__c.Locked__c)}">
                                        <a id='editClientSubject' class="pointer underline" style="padding-right: 5px;">Edit</a>
                                        <apex:outputLabel value="{!cs.Id}" styleClass="hidden"></apex:outputLabel>
                                        <apex:commandLink value="Del" action="{!DeleteClientSubject}" onclick="return confirmDelete();">
                                            <apex:param name="p1" assignTo="{!DelId}" value="{!cs.Id}"></apex:param>
                                        </apex:commandLink> 
<!--<apex:outputLink value="{!URLFOR($Action.Opportunity_Client_Subject__c.Delete, cs.Id,[retURL='/apex/HL_FEIS?id=' + OpportunityApproval.Id])}" target="_parent" onClick="return confirmDelete()">Del</apex:outputLink>-->
                                    </apex:outputPanel>
                                </apex:column>
                                <apex:column value="{!cs.Client_Subject__r.Name}" headerValue="Target/Subject Company Name"/>
                                <apex:column value="{!cs.Public_Or_Private__c}" headerValue="Public/Private"/>
                                <apex:column value="{!cs.Private_Equity_Ownership__c}" headerValue="Private Equity (%)"/>
                                <apex:column value="{!cs.Market_Cap_MM__c}" headerValue="Market Cap (Mil)"/>
                                <apex:column value="{!cs.ESOP__c}" headerValue="ESOP (%)"/>
                                <apex:column value="{!cs.NFP__c}" headerValue="Not for Profit (%)"/>
                                <apex:column value="{!cs.Insiders__c}" headerValue="Insiders (%)"/>
                                <apex:column headerValue="More Details" width="30px" styleClass="center">
                                    <apex:image styleClass="tipPopupTrigger pointer" alt="More Details" url="{!URLFOR($Resource.HL_Images,'Info.png')}" rendered="{!NOT ISBLANK(cs.Outside_Counsel__c) || NOT ISBLANK(cs.Financial_Advisors__c) || NOT ISBLANK(cs.Other_Financial_Advisor_Services__c)}" height="16px" title="More Details"/>
                                    <apex:outputPanel styleClass="popupDetails" style="display:none;">
                                        <apex:pageBlockSection columns="1" title="Additional Details for {!cs.Client_Subject__r.Name}" collapsible="false" >
