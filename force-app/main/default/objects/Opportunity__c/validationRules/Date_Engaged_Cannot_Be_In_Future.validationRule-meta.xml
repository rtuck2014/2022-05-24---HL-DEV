<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Engaged_Cannot_Be_In_Future</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK(Date_Engaged__c)) &amp;&amp; Date_Engaged__c &gt; TODAY() &amp;&amp; $User.ProfileId != &apos;00ei00000016T4B&apos;</errorConditionFormula>
    <errorDisplayField>Date_Engaged__c</errorDisplayField>
    <errorMessage>Date Engaged cannot be in the future.</errorMessage>
</ValidationRule>
