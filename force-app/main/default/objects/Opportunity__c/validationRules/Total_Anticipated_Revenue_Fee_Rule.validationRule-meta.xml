<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Anticipated_Revenue_Fee_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND($RecordType.DeveloperName=&quot;FAS&quot;,Total_Anticipated_Revenue__c &lt;  Total_Estimated_Fee__c)</errorConditionFormula>
    <errorMessage>Estimated Fees - Total Anticipated Revenue should be Greater Than or Equal to the Fee.</errorMessage>
</ValidationRule>
