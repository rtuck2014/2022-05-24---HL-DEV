<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>lmscons__Validate_Duration_max</fullName>
    <active>true</active>
    <description>If Duration number is too large.</description>
    <errorConditionFormula>lmscons__Duration__c &gt;= 1000000</errorConditionFormula>
    <errorDisplayField>lmscons__Duration__c</errorDisplayField>
    <errorMessage>Number is too large.</errorMessage>
</ValidationRule>
