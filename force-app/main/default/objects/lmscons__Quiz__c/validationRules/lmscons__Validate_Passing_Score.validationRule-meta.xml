<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>lmscons__Validate_Passing_Score</fullName>
    <active>true</active>
    <description>Passing Score must be between 0 and 100%.</description>
    <errorConditionFormula>OR(
lmscons__Passing_Score__c &lt; 0,
lmscons__Passing_Score__c &gt; 100
)</errorConditionFormula>
    <errorDisplayField>lmscons__Passing_Score__c</errorDisplayField>
    <errorMessage>Passing Score must be between 0 and 100%.</errorMessage>
</ValidationRule>
