<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NegativeTransactionValue</fullName>
    <active>true</active>
    <errorConditionFormula>IF(Transaction_Value__c &lt; 0, true, false)</errorConditionFormula>
    <errorMessage>Transaction Value cannot be Negative</errorMessage>
</ValidationRule>
