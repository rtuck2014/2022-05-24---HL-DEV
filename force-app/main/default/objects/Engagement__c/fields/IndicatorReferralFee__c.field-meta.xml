<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IndicatorReferralFee__c</fullName>
    <description>This formula field is being used in the &quot;Billing Request&quot; template, instead of showing the actual Referral Fee, this is shown, which is a Yes/No indicator.</description>
    <externalId>false</externalId>
    <formula>IF(
NOT(ISBLANK(Est_Referral_Fee__c)) &amp;&amp;
Est_Referral_Fee__c &gt; 0,
&quot;Yes&quot;,
&quot;No&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Indicator (Referral Fee)</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
