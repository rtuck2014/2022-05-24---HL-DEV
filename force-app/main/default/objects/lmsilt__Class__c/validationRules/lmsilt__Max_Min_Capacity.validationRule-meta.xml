<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>lmsilt__Max_Min_Capacity</fullName>
    <active>true</active>
    <errorConditionFormula>IF(AND(NOT(ISNULL(lmsilt__Maximum_Capacity__c)) ,NOT(ISNULL(lmsilt__Minimum_Capacity__c))), IF(lmsilt__Maximum_Capacity__c&lt;lmsilt__Minimum_Capacity__c, true, false), false)</errorConditionFormula>
    <errorMessage>Minimum capacity value exceeds Maximum capacity value.</errorMessage>
</ValidationRule>
