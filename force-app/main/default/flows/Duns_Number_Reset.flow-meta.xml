    <description>Reset D&amp;B Optimizer fields when match criteria changes</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>$User.Alias &lt;&gt; &quot;autocln&quot;  &amp;&amp; 

(ISCHANGED([Account].BillingStreet)|| ISCHANGED([Account].BillingCity) || ISCHANGED([Account].BillingState) || ISCHANGED([Account].BillingCountry) 
|| ISCHANGED([Account].BillingPostalCode)
)

&amp;&amp; 
( ISNULL([Account].DNBoptimizer__DNB_D_U_N_S_Number__c)= FALSE || ISNULL([Account].DNBoptimizer__DnBCompanyRecord__c)= FALSE
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>{!$User.Alias} &lt;&gt; &quot;autocln&quot;  &amp;&amp; 

(ISCHANGED({!myVariable_current.BillingStreet})|| ISCHANGED({!myVariable_current.BillingCity}) || ISCHANGED({!myVariable_current.BillingState}) || ISCHANGED({!myVariable_current.BillingCountry}) 
|| ISCHANGED({!myVariable_current.BillingPostalCode})
)

&amp;&amp; 
( ISNULL({!myVariable_current.DNBoptimizer__DNB_D_U_N_S_Number__c})= FALSE || ISNULL({!myVariable_current.DNBoptimizer__DnBCompanyRecord__c})= FALSE
)</expression>
    </formulas>
